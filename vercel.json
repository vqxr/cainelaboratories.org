{
  "version": 2,
  "name": "bbt-research",
  "buildCommand": null,
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/pipeline", "destination": "/pipeline.html" },
    { "source": "/math", "destination": "/math.html" },
    { "source": "/dataset", "destination": "/dataset.html" },
    { "source": "/console", "destination": "/console.html" }
  ]
}
